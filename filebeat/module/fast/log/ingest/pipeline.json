{
  "description": "Pipeline for parsing FAST logs",
  "processors": [
      {
          "set" : {
              "field" : "fast.log.meta.msg",
              "value" : "hovno",
              "ignore_failure" : false
          },
          "set" : {
              "field" : "fast.log.meta.message",
              "value" : "prdel",
              "ignore_failure" : false
          },
          "rename" : {
              "field" : "fast.log.meta.kubernetes.node.name",
              "target_field" : "fast.log.meta.kubernet.noud.nejm",
              "ignore_failure" : false
          }
      }
    ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
